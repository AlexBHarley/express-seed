{
  "name": "MeanieExpressSeed",
  "description": "Seed project for Express applications",
  "version": "1.0.1",
  "homepage": "https://github.com/meanie/express-seed",
  "author": {
    "name": "Adam Buczynski",
    "email": "me@adambuczynski.com",
    "url": "http://adambuczynski.com/"
  },
  "contributors": [],
  "license": "SEE LICENSE IN LICENSE.md",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/meanie/express-seed"
  },
  "bugs": {
    "url": "https://github.com/meanie/express-seed/issues"
  },
  "keywords": [],
  "engines": {
    "node": ">=5.3.0",
    "npm": ">=3.5.2"
  },
  "main": "scripts/server.js",
  "scripts": {
    "start": "node --es_staging --harmony_destructuring scripts/server.js",
    "dev": "nodemon --es_staging --harmony_destructuring scripts/server.js",
    "lint": "npm run lint:jshint -s && npm run lint:jscs -s",
    "lint:jshint": "jshint . --reporter=node_modules/jshint-stylish",
    "lint:jscs": "jscs . --reporter=node_modules/jscs-stylish",
    "pretest": "npm run lint -s",
    "istanbul": "babel-node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha",
    "test": "npm run istanbul -s",
    "coverage": "open -a \"Google Chrome\" ./coverage/lcov-report/index.html",
    "posttest": "npm run coverage -s",
    "preversion": "npm run test -s",
    "postversion": "git push && git push --tags",
    "migrate": "node --es_staging scripts/migrate.js",
    "refresh": "node --es_staging scripts/migrate.js refresh"
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.2.2",
    "body-parser": "^1.15.0",
    "chalk": "^1.1.1",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "entities": "^1.1.1",
    "express": "^4.13.4",
    "glob": "^6.0.4",
    "i18n": "^0.6.0",
    "jsonwebtoken": "^5.5.4",
    "moment": "^2.11.2",
    "mongoose": "^4.4.3",
    "morgan": "^1.6.1",
    "multer": "^1.1.0",
    "nodemailer": "^2.1.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "serve-static": "^1.10.2",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "dirty-chai": "^1.2.2",
    "express-simulate-latency": "0.0.2",
    "istanbul": "^1.0.0-alpha.2",
    "jscs": "^2.9.0",
    "jscs-stylish": "^0.3.1",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.1.0",
    "mocha": "^2.4.5",
    "mocha-clean": "^1.0.0",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.3",
    "sinon-mongoose": "^1.2.1"
  }
}
